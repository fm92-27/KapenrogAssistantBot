{
    "version": 2,
    "builds": [
        {
             "src": "index.js",
             "use": "@vercel/node" 
        },
        {
            "src": "bot.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "bot.js"
        }
    ]
}